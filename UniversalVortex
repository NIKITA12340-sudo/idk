local Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/weakhoes/Roblox-UI-Libs/refs/heads/main/Rayfield%20Lib/Rayfield%20Lib%20Source.lua"))()

local Window = Rayfield:CreateWindow({
    Name = "UniversalVortex",
    LoadingTitle = "Loading...",
    LoadingSubtitle = "Please wait...",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "UniversalVortex",
        FileName = "config"
    },
    KeySystem = false,
})

-- Welcome notification
Rayfield:Notify({
    Title = "Welcome!",
    Content = "Thank you for using my script!",
    Duration = 5
})

-- Creating Tabs
local MainTab = Window:CreateTab("Main", 4483365998)
local GameIDTab = Window:CreateTab("Game ID", 4483365998)
local PositionTab = Window:CreateTab("Position", 4483365998)
local ScriptsTab = Window:CreateTab("Scripts", 4483365998)
local InfoTab = Window:CreateTab("Info", 4483365998)
local TeleportTab = Window:CreateTab("Teleport to Games", 4483365998)

-- Main Tab
MainTab:CreateButton({
    Name = "Rejoin",
    Callback = function()
        local player = game.Players.LocalPlayer
        if player then
            player:Kick("Rejoining...")
            wait(1) -- wait a second before rejoining
            game:GetService("TeleportService"):Teleport(game.PlaceId, player)
        end
    end    
})

MainTab:CreateButton({
    Name = "Server Hop",
    Callback = function()
        local TeleportService = game:GetService("TeleportService")
        local player = game.Players.LocalPlayer
        TeleportService:Teleport(game.PlaceId, player)
    end    
})

MainTab:CreateSlider({
    Name = "Speed Hack",
    Range = {16, 100},
    Increment = 1,
    CurrentValue = 16,
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
    end
})

MainTab:CreateSlider({
    Name = "Jump Height",
    Range = {50, 300},
    Increment = 5,
    CurrentValue = 50,
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end
})

MainTab:CreateSlider({
    Name = "Gravity",
    Range = {0, 196.2},
    Increment = 0.1,
    CurrentValue = 196.2,
    Callback = function(Value)
        game.Workspace.Gravity = Value
    end
})

-- Game ID Tab
local gameId = game.PlaceId
GameIDTab:CreateLabel("Game ID: " .. gameId)

GameIDTab:CreateButton({
    Name = "Copy Game ID",
    Callback = function()
        setclipboard(gameId)
        Rayfield:Notify({ Title = "Copied!", Content = "Game ID copied: " .. gameId, Duration = 2 })
    end    
})

GameIDTab:CreateButton({
    Name = "Copy Teleport Code",
    Callback = function()
        local teleportCode = "game:GetService('TeleportService'):Teleport(" .. gameId .. ", game.Players.LocalPlayer)"
        setclipboard(teleportCode)
        Rayfield:Notify({ Title = "Copied!", Content = "Teleport code copied: " .. teleportCode, Duration = 2 })
    end    
})

-- Position Tab
local positionLabel = PositionTab:CreateLabel("Current Position: (0, 0, 0)")
local autoCoordsEnabled = false
local positionUpdate

PositionTab:CreateButton({
    Name = "Copy Current Position",
    Callback = function()
        local playerPosition = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        if playerPosition then
            local positionCode = "game.Players.LocalPlayer.Character:MoveTo(Vector3.new(" .. playerPosition.X .. ", " .. playerPosition.Y .. ", " .. playerPosition.Z .. "))"
            setclipboard(positionCode)
            Rayfield:Notify({ Title = "Copied!", Content = "Current position code copied: " .. positionCode, Duration = 2 })
        else
            Rayfield:Notify({ Title = "Error!", Content = "Character not found or position unavailable.", Duration = 2 })
        end
    end    
})

PositionTab:CreateToggle({
    Name = "Enable Auto Coordinates",
    CurrentValue = false,
    Flag = "AutoCoords",
    Callback = function(state)
        autoCoordsEnabled = state
        if autoCoordsEnabled then
            positionLabel:Set("Auto Coordinates: Enabled")
            positionUpdate = game:GetService("RunService").Heartbeat:Connect(function()
                local playerPosition = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character.HumanoidRootPart.Position
                if playerPosition then
                    positionLabel:Set("Current Position: " .. tostring(playerPosition))
                end
            end)
        else
            positionLabel:Set("Current Position: (0, 0, 0)")
            if positionUpdate then
                positionUpdate:Disconnect()
            end
        end
    end    
})

PositionTab:CreateLabel("Position Info: Track your current location in the game.")

-- Tween Function for Position
local TweenService = game:GetService("TweenService")

local function tweenToPosition(targetPosition)
    local character = game.Players.LocalPlayer.Character
    if character and character:FindFirstChild("HumanoidRootPart") then
        local humanoidRootPart = character.HumanoidRootPart
        local tweenInfo = TweenInfo.new(1, Enum.EasingStyle.Quad, Enum.EasingDirection.Out) -- 1 second duration
        local goal = {Position = targetPosition}
        local tween = TweenService:Create(humanoidRootPart, tweenInfo, goal)
        tween:Play()
    end
end

PositionTab:CreateButton({
    Name = "Teleport to Current Position",
    Callback = function()
        local playerPosition = game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character.HumanoidRootPart.Position
        if playerPosition then
            tweenToPosition(playerPosition)
        else
            Rayfield:Notify({ Title = "Error!", Content = "Character not found or position unavailable.", Duration = 2 })
        end
    end    
})

-- Scripts Tab
ScriptsTab:CreateButton({
    Name = "Fly V3",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/XNEOFF/FlyGuiV3/main/FlyGuiV3.txt"))()
    end    
})

ScriptsTab:CreateButton({
    Name = "Dark Dex V4",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/NIKITA12340-sudo/dermo/refs/heads/Script/Dex%20v4(NOTMINE)"))()
    end    
})

ScriptsTab:CreateButton({
    Name = "Infinity Yield",
    Callback = function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source"))()
    end    
})

-- Info Tab
InfoTab:CreateLabel("Script made by VerseVortex")
InfoTab:CreateLabel("Script was made by ChatGPT and may have some bugs")
